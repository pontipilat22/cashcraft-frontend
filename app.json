{
  "expo": {
    "name": "CashCraft",
    "slug": "cashcraft3",
    "version": "1.0.0",
    "description": "Управляйте финансами легко",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "packagerOpts": {
      "port": 8081
    },
    "scheme": "cashcraft",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4287f5"
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.pontipilat.cashcraft",
      "buildNumber": "1",
      "scheme": "cashcraft",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.YOUR_IOS_CLIENT_ID"
        }
      },
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#4287f5"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pontipilat.cashcraft",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "cashcraft"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#4287f5"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "993876aa-d5bf-411d-b069-94f585de32af"
      },
      "googleWebClientId": "457720015497-gkarur46ep22kptgra8qm4v58r686jab.apps.googleusercontent.com",
      "googleAndroidClientId": "457720015497-phj9gjn84anqsnoufvv6bro3ca9oud03.apps.googleusercontent.com",
      "googleIosClientId": "YOUR_IOS_CLIENT_ID.apps.googleusercontent.com",
      "scheme": "cashcraft"
    }
  }
}
