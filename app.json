{
  "expo": {
    "name": "Cashcraft",
    "slug": "cashcraft3",
    "version": "1.0.0",
    "description": "Управляйте финансами легко",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "packagerOpts": {
      "port": 8081
    },
    "scheme": "cashcraft",
    "splash": {
      "image": "./assets/splash-icon1.png",
      "resizeMode": "contain",
      "backgroundColor": "#EAF4FF",
      "dark": {
        "image": "./assets/splash-icon1.png",
        "resizeMode": "contain",
        "backgroundColor": "#0B1220"
      }
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.pontipilat.cashcraft",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.pontipilat.cashcraft",
      "versionCode": 3,
      "edgeToEdgeEnabled": true,
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4287f5"
      },
      "splash": {
        "image": "./assets/splash-icon1.png",
        "resizeMode": "center",
        "backgroundColor": "#EAF4FF",
        "dark": {
          "image": "./assets/splash-icon1.png",
          "resizeMode": "center",
          "backgroundColor": "#0B1220"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "cashcraft"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-iap",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.457720015497-urkrpt05vv1q1vtdbps1fnancbb984of"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "993876aa-d5bf-411d-b069-94f585de32af"
      },
      "googleWebClientId": "457720015497-e98pchk5nluung53cjckq960qvjfdm7b.apps.googleusercontent.com",
      "googleAndroidClientId": "457720015497-8j7ru4jrrec7qe0l53pv0c8tkfnuv1ob.apps.googleusercontent.com",
      "googleIosClientId": "457720015497-0ac2v6rkqnb1gbv63vt44pvpv0g2igk9.apps.googleusercontent.com"
    }
  }
}
